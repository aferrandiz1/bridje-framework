<?xml version="1.0" encoding="UTF-8"?>
<hmodel 
    xmlns="http://www.bridje.org/schemas/hmodel" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bridje.org/schemas/hmodel file:/E:/Projects/bridje-framework/bridje-data/target/generated-resources/data/BRIDJE-INF/schemas/hmodel.xsd"
    name="HModel" packageName="org.bridje.data.hmodel" extendsFrom="HEntityData" 
    namespace="http://www.bridje.org/schemas/hmodel">

