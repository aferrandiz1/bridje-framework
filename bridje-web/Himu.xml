<?xml version="1.0" encoding="UTF-8"?>
<uisuite
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://www.bridje.org/schemas/web/uisuite'
    xsi:schemaLocation='http://www.bridje.org/schemas/web/uisuite https://oss.sonatype.org/content/repositories/snapshots/org/bridje/bridje-web/0.3.1-SNAPSHOT/bridje-web-0.3.1-20170328.231052-21-uisuite.xsd'
    name="Himu"
    package="com.touwolf.themes.bootstrap.himu" 
    namespace="http://www.touwolf.com/themes/himu">

    <renderHead>
        <![CDATA[
            <head>
                <title>${view.title!}</title>
                <meta charset="UTF-8">
                <meta name="viewport" content="width=device-width, initial-scale=1.0">
                <#nested />
                <!--[if lt IE 9]>
                <script src="__themes/himu/resources/js/html5shiv.js"></script> 
                <script src="__themes/himu/resources/js/respond.min.js"></script>
                <![endif]-->
                <link rel="shortcut icon" href="images/ico/favicon.png"> 
                <link rel="apple-touch-icon-precomposed" sizes="144x144" href="images/ico/apple-touch-icon-144-precomposed.png"> 
                <link rel="apple-touch-icon-precomposed" sizes="114x114" href="images/ico/apple-touch-icon-114-precomposed.png"> 
                <link rel="apple-touch-icon-precomposed" sizes="72x72" href="images/ico/apple-touch-icon-72-precomposed.png"> 
                <link rel="apple-touch-icon-precomposed" href="images/ico/apple-touch-icon-57-precomposed.png">
            </head>
        ]]>
    </renderHead>
    <renderBody>
        <![CDATA[
            <body>
                <#nested />
            </body>
        ]]>
    </renderBody>
    <renderViewContainer>
        <![CDATA[
            <div id="view-form">
                <#nested />
            </div>
        ]]>
    </renderViewContainer>

    <standalone>
        <child name="page" type="Page" />
    </standalone>
    <defines>
        
    </defines>

    <defaultResources>
        <style href="css/bootstrap.min.css" />
        <style href="css/prettyPhoto.css" />
        <style href="css/font-awesome.min.css" />
        <style href="css/animate.css" />
        <style href="css/main.css" />
        <style href="css/responsive.css" />
	<script href="js/jquery.js"></script> 
	<script href="js/bootstrap.min.js"></script>
	<script href="js/smoothscroll.js"></script> 
	<script href="js/jquery.isotope.min.js"></script>
	<script href="js/jquery.prettyPhoto.js"></script> 
	<script href="js/jquery.parallax.js"></script> 
	<script href="js/main.js"></script> 
        <script href="js/script.js" />
    </defaultResources>

    <controls>
        <control name="Logo" >
            <fields>
                <attr name="href" type="String" />
                <attr name="image" type="String" />
            </fields>
            <render>
                <![CDATA[
                    <a class="navbar-brand" href="${control.href!}"><h1><img src="${control.image!}" alt="logo"></h1></a>
                ]]>
            </render>
        </control>
        <control name="Navigation" >
            <fields>
                <child name="logo" type="Logo" />
            </fields>
            <render>
                <![CDATA[
                    <header id="navigation"> 
                        <div class="navbar navbar-inverse navbar-fixed-top" role="banner"> 
                            <div class="container"> 
                                <div class="navbar-header"> 
                                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse"> 
                                        <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> 
                                    </button> 
                                    <@renderControl control.logo />
                                </div> 
                                <div class="collapse navbar-collapse"> 
                                    <ul class="nav navbar-nav navbar-right">
                                        <#if pageControl.home?? >
                                            <li class="scroll active"><a href="#navigation">Home</a></li> 
                                        </#if>
                                        <#if pageControl.aboutus?? >
                                            <li class="scroll"><a href="#about-us">About Us</a></li> 
                                        </#if>
                                        <#if pageControl.services?? >
                                            <li class="scroll"><a href="#services">Services</a></li>
                                        </#if>
                                        <#if pageControl.ourteam?? >
                                            <li class="scroll"><a href="#our-team">Our Team</a></li> 
                                        </#if>
                                        <#if pageControl.portfolio?? >
                                            <li class="scroll"><a href="#portfolio">Portfolio</a></li> 
                                        </#if>
                                        <#if pageControl.clients?? >
                                            <li class="scroll"><a href="#clients">Clients</a></li> 
                                        </#if>
                                        <#if pageControl.blog?? >
                                            <li class="scroll"><a href="#blog">Blog</a></li> 
                                        </#if>
                                        <#if pageControl.contact?? >
                                            <li class="scroll"><a href="#contact">Contact</a></li>
                                        </#if>
                                    </ul> 
                                </div> 
                            </div> 
                        </div>
                    </header>
                ]]>
            </render>
        </control>
        <control name="Link" >
            <fields>
                <outAttr name="href" type="String" />
                <outValue name="value" type="String" />
            </fields>
            <render>
                <![CDATA[
                    <a class="${linkCssClasses!}" href="${control.href!}">${control.value!}</a> 
                ]]>
            </render>
        </control>
        <control name="CarouselSlide" >
            <fields>
                <outAttr name="image" type="String" />
                <outAttr name="title" type="String" />
                <outAttr name="content" type="String" />
                <outAttr name="active" type="Boolean" def="false" />
                <child name="link" type="Link" />
            </fields>
            <render>
                <![CDATA[
                    <div class="item <#if control.active>active</#if>" style="background-image: url(${control.image!})"> 
                        <div class="carousel-caption"> 
                            <div> 
                                <h2 class="heading animated bounceInDown">${control.title!}</h2> 
                                <p class="animated bounceInUp">${control.content!}</p> 
                                <#assign linkCssClasses = "btn btn-default slider-btn animated fadeIn" />
                                <#if control.link??>
                                    <@renderControl control.link />
                                </#if>
                            </div> 
                        </div> 
                    </div>
                ]]>
            </render>
        </control>
        <control name="Home" >
            <fields>
                <children name="slides">
                    <child name="slide" type="CarouselSlide" />
                </children>
            </fields>
            <render>
                <![CDATA[
                    <section id="home">
                        <div class="home-pattern"></div>
                        <div id="main-carousel" class="carousel slide" data-ride="carousel"> 
                            <ol class="carousel-indicators">
                                <#list control.slides as slide>
                                    <li data-target="#main-carousel" data-slide-to="${slide_index}" <#if slide.active>class="active"</#if> ></li>
                                </#list>
                            </ol>
                            <div class="carousel-inner">
                                <#list control.slides as slide>
                                    <@renderControl slide />
                                </#list>
                            </div>

                            <a class="carousel-left member-carousel-control hidden-xs" href="#main-carousel" data-slide="prev"><i class="fa fa-angle-left"></i></a>
                            <a class="carousel-right member-carousel-control hidden-xs" href="#main-carousel" data-slide="next"><i class="fa fa-angle-right"></i></a>
                        </div>
                    </section>
                ]]>
            </render>
        </control>
        <control name="AboutUsTab" >
            <fields>
                <outAttr name="id" type="String" def='"sometab"' />
                <outAttr name="active" type="Boolean" def='false' />
                <outAttr name="title" type="String" def='""' />
                <outEl name="description" type="String" def='""' />
                <outAttr name="image" type="String" def='"#"' />
                <outAttr name="icon" type="String" def='"#"' />
                <outAttr name="alt" type="String" def='""' />
            </fields>
            <render>
                <![CDATA[
                    <div class="tab-pane fade in <#if control.active>active</#if>" id="${control.id!}">
                        <div class="media">
                            <img class="pull-left media-object" src="${control.image!}" alt="${control.alt!}"> 
                            <div class="media-body">
                                <p>${control.description!}</p>
                            </div>
                        </div>
                    </div>
                ]]>
            </render>
        </control>
        <control name="AboutUsTabs" >
            <fields>
                <outAttr name="title" type="String" def='""' />
                <children name="tabs" >
                    <child name="tab" type="AboutUsTab" />
                </children>
            </fields>
            <render>
                <![CDATA[
                    <div class="col-sm-6">
                        <h3>${control.title!}</h3>
                        <ul class="nav nav-tabs">
                            <#list control.tabs as tab>
                                <li <#if tab.active>class="active"</#if>><a href="#${tab.id!}" data-toggle="tab"><i class="fa ${tab.icon!}"></i> ${tab.title!}</a></li>
                            </#list>
                        </ul>
                        <div class="tab-content">
                            <#list control.tabs as tab>
                                <@renderControl tab />
                            </#list>
                        </div>
                    </div>
                ]]>
            </render>
        </control>
        <control name="AboutUsBar" >
            <fields>
                <outAttr name="title" type="String" def='""' />
                <outAttr name="percent" type="Integer" def='0' />
            </fields>
            <render>
                <![CDATA[
                    <div class="skillbar clearfix " data-percent="${control.percent?string.computer}%">
                        <div class="skillbar-title">
                            <span>${control.title!}</span>
                        </div>
                        <div class="skillbar-bar"></div>
                        <div class="skill-bar-percent">${control.percent?string.computer}%</div>
                    </div>
                ]]>
            </render>
        </control>
        <control name="AboutUsBars" >
            <fields>
                <outAttr name="title" type="String" def='""' />
                <children name="bars" >
                    <child name="bar" type="AboutUsBar" />
                </children>
            </fields>
            <render>
                <![CDATA[
                    <div class="col-sm-6">
                        <h3>${control.title!}</h3>
                        <div class="skill-bar">
                            <#list control.bars as bar>
                                <@renderControl bar />
                            </#list>
                        </div>
                    </div>
                ]]>
            </render>
        </control>
        <control name="AboutUs" >
            <fields>
                <outAttr name="title" type="String" def='""' />
                <outEl name="description" type="String" def='""' />
                <child name="tabs" type="AboutUsTabs" />
                <child name="bars" type="AboutUsBars" />
            </fields>
            <render>
                <![CDATA[
                    <section id="about-us">
                        <div class="container">
                            <div class="text-center">
                                <div class="col-sm-8 col-sm-offset-2">
                                    <h2 class="title-one">${control.title!}</h2>
                                    <p>${control.description!}</p>
                                </div>
                            </div>
                            <div class="about-us">
                                <div class="row">
                                    <#if control.tabs??>
                                        <@renderControl control.tabs />
                                    </#if>
                                    <#if control.bars??>
                                        <@renderControl control.bars />
                                    </#if>
                                </div>
                            </div>
                        </div>
                    </section>
                ]]>
            </render>
        </control>
        <control name="ServiceBlock" >
            <fields>
                <outAttr name="title" type="String" def='"Modern Design"' />
                <outAttr name="icon" type="String" def='"fa-th"' />
                <outEl name="description" type="String" def='"Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat. Ut wisi enim ad minim veniam, quis nostrud exerci tation ullamcorper suscipit lobortis nisl ut aliquip ex ea commodo consequat. Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore."' />
            </fields>
            <render>
                <![CDATA[
                    <div class="col-sm-4">
                        <div class="single-service">
                            <i class="fa ${control.icon!}"></i>
                            <h2>${control.title!}</h2>
                            <p>${control.description!}</p>
                        </div>
                    </div>
                ]]>
            </render>
        </control>
        <control name="Services" >
            <fields>
                <outAttr name="title" type="String" def='"Services"' />
                <outEl name="description" type="String" def='"Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat. Ut wisi enim ad minim veniam, quis nostrud exerci tation ullamcorper suscipit lobortis nisl ut aliquip ex ea commodo consequat."' />
                <children name="services" >
                    <child name="service" type="ServiceBlock" />
                </children>
            </fields>
            <render>
                <![CDATA[
                    <section id="services" class="parallax-section">
                        <div class="container">
                            <div class="row text-center">
                                <div class="col-sm-8 col-sm-offset-2">
                                    <h2 class="title-one">${control.title!}</h2>
                                    <p>${control.description!}</p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="our-service">
                                        <div class="services row">
                                            <@renderAllControls control.services![] />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                ]]>
            </render>
        </control>
        <control name="OurTeamRow" >
            <fields>
                <outAttr name="active" type="Boolean" def="false" />
                <children name="members">
                    <child name="member" type="OurTeamMember" />
                </children>
            </fields>
            <render>
                <![CDATA[
                    <div class="row item <#if control.active>active</#if>">
                        <@renderAllControls control.members![] />
                    </div>
                ]]>
            </render>
        </control>
        <control name="OurTeamMemberSocial" >
            <fields>
                <outEl name="facebook" type="String" def='"#"' />
                <outEl name="twitter" type="String" def='"#"' />
                <outEl name="googlePlus" type="String" def='"#"' />
                <outEl name="dribbble" type="String" def='"#"' />
                <outEl name="linkedin" type="String" def='"#"' />
            </fields>
            <render>
                <![CDATA[
                    <div class="socials">
                        <a href="${control.facebook!}"><i class="fa fa-facebook"></i></a>
                        <a href="${control.twitter!}"><i class="fa fa-twitter"></i></a>
                        <a href="${control.googlePlus!}"><i class="fa fa-google-plus"></i></a>
                        <a href="${control.dribbble!}"><i class="fa fa-dribbble"></i></a>
                        <a href="${control.linkedin!}"><i class="fa fa-linkedin"></i></a>
                    </div>
                ]]>
            </render>
        </control>
        <control name="OurTeamMember" >
            <fields>
                <outAttr name="image" type="String" def='"images/our-team/member1.jpg"' />
                <outAttr name="name" type="String" def='"William Hurt"' />
                <outAttr name="job" type="String" def='"Sr. Web Developer"' />
                <outAttr name="alt" type="String" def='"team member"' />
                <outEl name="description" type="String" def='"Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod"' />
                <child name="socials" type="OurTeamMemberSocial" />
            </fields>
            <render>
                <![CDATA[
                    <div class="col-sm-6 col-md-3">
                        <div class="single-member">
                            <img src="${control.image!}" alt="${control.alt!}" />
                            <h4>${control.name!}</h4>
                            <h5>${control.job!}</h5>
                            <p>${control.description!}</p>
                            <@renderControl control.socials![] />
                        </div>
                    </div>
                ]]>
            </render>
        </control>
        <control name="OurTeam" >
            <fields>
                <outAttr name="title" type="String" def='"Meet The Team"' />
                <outEl name="description" type="String" def='"Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat. Ut wisi enim ad minim veniam, quis nostrud exerci tation ullamcorper suscipit."' />
                <children name="rows">
                    <child name="row" type="OurTeamRow" />
                </children>
            </fields>
            <render>
                <![CDATA[
                    <section id="our-team">
                        <div class="container">
                            <div class="row text-center">
                                <div class="col-sm-8 col-sm-offset-2">
                                    <h2 class="title-one">${control.title!}</h2>
                                    <p>${control.description!}</p>
                                </div>
                            </div>
                            <div id="team-carousel" class="carousel slide" data-interval="false">
                                <a class="member-left" href="#team-carousel" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
                                <a class="member-right" href="#team-carousel" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
                                <div class="carousel-inner team-members">
                                    <@renderAllControls control.rows![] />
                                </div>
                            </div>
                        </div>
                    </section>
                ]]>
            </render>
        </control>
        <control name="PortfolioItem" >
            <fields>
                <outAttr name="category" type="String" def='"html"' />
                <outAttr name="image" type="String" def='"__themes/himu/images/portfolio/1.jpg"' />
                <outAttr name="title" type="String" def='"Novel"' />
                <outAttr name="description" type="String" def='"Lorem ipsum dolor sit amet consectetur"' />
            </fields>
            <render>
                <![CDATA[
                    <div class="col-sm-3 col-xs-12 portfolio-item ${control.category!}">
                        <div class="view efffect">
                            <div class="portfolio-image">
                                <img src="${control.image!}" alt="">
                            </div>
                            <div class="mask text-center">
                                <h3>${control.title!}</h3>
                                <h4>${control.description!}</h4>
                                <a href="#"><i class="fa fa-link"></i></a>
                                <a href="__themes/himu/images/portfolio/big-item.jpg" data-gallery="prettyPhoto"><i class="fa fa-search-plus"></i></a>
                            </div>
                        </div>
                    </div>
                ]]>
            </render>
        </control>
        <control name="PortfolioCategory" >
            <fields>
                <outAttr name="name" type="String" def='"html"' />
                <outAttr name="title" type="String" def='"Html"' />
            </fields>
            <render>
		<![CDATA[
                    <li><a class="btn btn-default" href="#" data-filter=".${control.name!}">${control.title!}</a></li>
		]]>
            </render>
        </control>
        <control name="Portfolio" >
            <fields>
                <outAttr name="title" type="String" def='"Portfolio"' />
                <outEl name="description" type="String" def='"Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat. Ut wisi enim ad minim veniam, quis nostrud exerci tation ullamcorper suscipit."' />
                <children name="items" wrapper="items" >
                    <child name="item" type="PortfolioItem" />
                </children>
                <children name="categories" wrapper="categories" >
                    <child name="category" type="PortfolioCategory" />
                </children>
            </fields>
            <render>
                <![CDATA[
                    <section id="portfolio">
                        <div class="container">
                            <div class="row text-center">
                                <div class="col-sm-8 col-sm-offset-2">
                                    <h2 class="title-one">${control.title!}</h2>
                                    <p>${control.description!}</p>
                                </div>
                            </div>
                            <ul class="portfolio-filter text-center">
                                <li><a class="btn btn-default active" href="#" data-filter="*">All</a></li>
                                <@renderAllControls control.categories![] />
                            </ul>
                            <div class="portfolio-items">
                                <@renderAllControls control.items![] />
                            </div>
                        </div> 

                    </section>
                ]]>
            </render>
        </control>
        <control name="Client" >
            <fields>
                <outAttr name="active" type="Boolean" def="false" />
                <outAttr name="image" type="String" def='""' />
                <outEl name="description" type="String" def='"Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer posuere erat a ante. Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer posuere erat a ante."' />
                <outAttr name="subtitle" type="String" def='""' />
                <outAttr name="link" type="String" def='"www.yourwebsite.com"' />
            </fields>
            <render>
		<![CDATA[
                    <div class="item <#if control.active>active</#if>">
                        <div class="single-client">
                            <div class="media">
                                <img class="pull-left" src="${control.image!}" alt="">
                                <div class="media-body">
                                    <blockquote>
                                        <p>${control.description!}</p>
                                        <small>${control.subtitle!}</small>
                                        <a href="${control.link!}">${control.link!}</a>
                                    </blockquote>
                                </div>
                            </div>
                        </div>
                    </div>
		]]>
            </render>
        </control>
        <control name="Clients" >
            <fields>
                <outAttr name="title" type="String" def='"Clients Say About Us"' />
                <outEl name="description" type="String" def='"Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat. Ut wisi enim ad minim veniam, quis nostrud exerci tation ullamcorper suscipit. Lorem ipsum dolor sit amet, consectetuer adipiscing elit"' />
                <children name="clients">
                    <child name="client" type="Client" />
                </children>
            </fields>
            <render>
                <![CDATA[
                    <section id="clients" class="parallax-section">
                        <div class="container">
                            <div class="clients-wrapper">
                                <div class="row text-center">
                                    <div class="col-sm-8 col-sm-offset-2">
                                        <h2 class="title-one">${control.title!}</h2>
                                        <p>${control.description!}</p>
                                    </div>
                                </div>
                                <div id="clients-carousel" class="carousel slide" data-ride="carousel">
                                    <ol class="carousel-indicators">
                                        <#list control.clients as client>
                                            <li data-target="#clients-carousel" data-slide-to="${client_index}" <#if client_index == 0>class="active"</#if> ></li>
                                        </#list>
                                    </ol>
                                    <div class="carousel-inner">
                                        <@renderAllControls control.clients![] />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                ]]>
            </render>
        </control>
        <control name="BlogPost" >
            <fields>
                <outAttr name="image" type="String" def='"__themes/himu/images/blog/1.jpg"' />
                <outAttr name="title" type="String" def='"Lorem ipsum dolor sit amet"' />
                <outAttr name="author" type="String" def='"John"' />
                <outAttr name="date" type="String" def='"Apr 15 2014"' />
                <outEl name="description" type="String" def='"Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat."' />
                <outAttr name="link" type="String" def='"#"' />
            </fields>
            <render>
		<![CDATA[
                    <div class="col-sm-4">
                        <div class="single-blog">
                            <img src="${control.image!}" alt="" />
                            <h2>${control.title!}</h2>
                            <ul class="post-meta">
                                <li><i class="fa fa-pencil-square-o"></i><strong> Posted By:</strong> ${control.author!}</li>
                                <li><i class="fa fa-clock-o"></i><strong> Posted On:</strong> ${control.date!}</li>
                            </ul>
                            <div class="blog-content">
                                <p>${control.description!}</p>
                            </div>
                            <a href="${control.link!}" class="btn btn-primary" data-toggle="modal" data-target="#blog-detail">Read More</a>
                        </div>
                        <div class="modal fade" id="blog-detail" tabindex="-1" role="dialog" aria-hidden="true">
