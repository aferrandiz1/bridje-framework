<?xml version="1.0" encoding="UTF-8"?>
<datatypes
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://www.bridje.org/schemas/orm/datatypes'
    xsi:schemaLocation='http://www.bridje.org/schemas/orm/datatypes custom-datatypes.xsd'>
    <datatype name="LocalDate" class="java.time.LocalDate" adapter="org.bridje.orm.adapters.LocalDateAdapter" sqlType="DATE" columnType="TableComparableColumn" />
    <datatype name="LocalTime" class="java.time.LocalTime" adapter="org.bridje.orm.adapters.LocalTimeAdapter" sqlType="TIME" columnType="TableComparableColumn" />
    <datatype name="LocalDateTime" class="java.time.LocalDateTime" adapter="org.bridje.orm.adapters.LocalDateTimeAdapter" sqlType="TIMESTAMP" columnType="TableComparableColumn" />
</datatypes>
